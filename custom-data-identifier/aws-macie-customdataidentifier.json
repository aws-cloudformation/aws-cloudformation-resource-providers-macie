{
  "typeName": "AWS::Macie::CustomDataIdentifier",
  "description": "The AWS::Macie::CustomDataIdentifier resource specifies a new CustomDataIdentifier. A CustomDataIdentifier allows users to detect data matching specific criteria in their ClassificationJob.",
  "sourceUrl": "https://github.com/aws-cloudformation/aws-cloudformation-rpdk.git",
  "properties": {
    "Name": {
      "description": "Name of the CustomDataIdentifier.",
      "type": "string"
    },
    "Description": {
      "description": "Description of the CustomDataIdentifier.",
      "type": "string"
    },
    "CustomDataIdentifierId": {
      "description": "Unique identifier of the CustomDataIdentifier",
      "type": "string"
    },
    "Regex": {
      "description": "The title of the TPS report is a mandatory element.",
      "type": "string"
    },
    "MaximumMatchDistance": {
      "description": "Required for all TPS Reports submitted after 2/19/1999",
      "type": "integer"
    },
    "Keywords": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "IgnoreList": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "Name",
    "Title"
  ],
  "readOnlyProperties": [
    "/properties/CustomDataIdentifierId"
  ],
  "primaryIdentifier": [
    "/properties/CustomDataIdentifierId"
  ],
  "handlers": {
    "create": {
      "permissions": [
        "macie2:CreateCustomDataIdentifier"
      ]
    },
    "read": {
      "permissions": [
        "macie2:GetCustomDataIdentifiers"
      ]
    },
    "delete": {
      "permissions": [
        "initech:DeleteCustomDataIdentifier"
      ]
    },
    "list": {
      "permissions": [
        "macie2:ListCustomDataIdentifiers",
        "macie2:GetCustomDataIdentifiers"
      ]
    }
  }
}
